										value matches {True}
									}
								}
							}
							ELEMENT[at0018] occurrences matches {0..1} matches {	-- Tender
								value matches {
									DV_BOOLEAN matches {
										value matches {True}
									}
								}
							}
							allow_archetype CLUSTER[at0021] occurrences matches {0..*} matches {	-- Related Examination Detail
								include
									archetype_id/value matches {/.*/}
							}
						}
					}
