							}
						}
					}
					CLUSTER[at0015] occurrences matches {0..*} matches {	-- Detailed Examination
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0016] occurrences matches {0..1} matches {	-- Pinna Structure
								value matches {
